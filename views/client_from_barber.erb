<h2>Список записавшихся клиентов</h2>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Phone</th>
      <th scope="col">DateStamp</th>
      <th scope="col">Barber</th>
      <th scope="col">Color</th>
    </tr>
  </thead>
  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%=client.id%></td>
        <td><%=client.name%></td>
        <td><%=client.phone%></td>
        <td><%=client.datestamp%></td>
        <td><%=client.barber%></td>
        <td><%=client.color%></td>
      </tr>
    <% end %>
  </tbody>
</table>